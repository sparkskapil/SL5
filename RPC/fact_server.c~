#include "fact.h"

int factorial(int n){
	if(n<=1)return 1;
	return factorial(n-1)*n;
}

int *
fact_1_svc(intpair *argp, struct svc_req *rqstp)
{
	static int  result;
	result = factorial(argp->a)

	return &result;
}
